<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteBase /
  RewriteRule ^index\.html$ - [L]
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteRule . /index.html [L]
  RewriteEngine On
  RewriteCond %{HTTP_HOST} ^nanoquesttech\.in$ [NC]
  RewriteRule ^(.*)$ https://www.nanoquesttech.in/$1 [R=301,L]

</IfModule>

